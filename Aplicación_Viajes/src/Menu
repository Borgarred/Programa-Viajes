package Aplicacion_Viajes;

//Clase abstracta Men√∫ que hereda de Usuario
public abstract class Menu extends Usuario {
//Metodos abstractos
	public abstract void MenuInicio();

	public abstract void MenuViajes();

	public abstract void menuDestino();

	public abstract void menuFecha();

	public abstract void menuRecomendaciones();

	public abstract void menuSorteo();
}
