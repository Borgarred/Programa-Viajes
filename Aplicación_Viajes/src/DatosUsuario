package Aplicacion_Viajes;
import java.util.Scanner;

public class DatosUsuario {

	public String nombre;
	public String apellidos;
	public String DNI;
	public int edad;
	
	public DatosUsuario() {
		
	}
	
	public DatosUsuario(String nombre, String apellidos, String DNI, int edad) {
		this.nombre = nombre;
		this.apellidos = apellidos;
		this.DNI = DNI;
		this.edad = edad;
	}
	
	public String getNombre() {
		return this.nombre;
	}

}
